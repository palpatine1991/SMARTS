<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>smarts</servlet-name>
        <servlet-class>SMARTS.servlet.smarts</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>smarts</servlet-name>
        <url-pattern>/smarts</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>progress</servlet-name>
        <servlet-class>SMARTS.servlet.progress</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>progress</servlet-name>
        <url-pattern>/progress</url-pattern>
    </servlet-mapping>
</web-app>
